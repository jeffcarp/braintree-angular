  },
  "keywords": [
    "angular",
    "braintree"
  ],
  "author": "Braintree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffcarp/braintree-angular/issues"
  },
  "homepage": "https://github.com/jeffcarp/braintree-angular",
  "dependencies": {
    "braintree-web": "3.6.3"
  },
  "devDependencies": {
    "angular": "^1.3.3",
    "body-parser": "^1.9.3",
    "braintree": "^1.21.0",
    "browserify": "^13.0.1",
    "express": "^4.10.4",
    "mocha": "^2.0.1",
    "phantom": "^2.1.2",
    "standard": "^8.0.0"
  },
  "standard": {
    "ignore": [
      "*.dist.js"
